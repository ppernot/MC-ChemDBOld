@ARTICLE{AAgren2009,
  author = {K. {\AA gren} and J.-E. Wahlund and P. Garnier and R. Modolo and
	J. Cui and M. Galand and I. M\"uller-Wodarg},
  title = {On the ionospheric structure of {T}itan},
  journal = {Planetary and Space Science},
  year = {2009},
  volume = {57},
  pages = {1821 -- 1827},
  number = {14-15},
  abstract = {In this study we present data from 17 Titan flybys showing that solar
	photons are the main ionisation source of Titan's dayside atmosphere.
	This is the first comprehensive study of Solar Zenith Angle (SZA)
	dependence of the electron number density and electron temperature
	at the ionospheric peak. The results show on average four times more
	plasma on the dayside compared to the nightside, with typical dayside
	electron densities of around 2500- and corresponding nightside densities
	of around 400-. We identify a broad transition region between SZA
	50o and 100o, where the ionosphere of Titan changes from being entirely
	sunlit to being in the shadow of the moon. For SZA <50o the ionisation
	peak altitude increases with increasing SZA, whereas the transition
	region and the nightside show more scattered ionospheric peak altitudes.
	Typical electron temperatures at the ionospheric peak are 0.03-0.06 eV
	([approximate]350-700 K) for both dayside and nightside.},
  doi = {10.1016/j.pss.2009.04.012},
  file = {Agren2009.pdf:Agren2009.pdf:},
  issn = {0032-0633},
  keywords = {Titan},
  owner = {plessis},
  timestamp = {2009.11.27}
}

